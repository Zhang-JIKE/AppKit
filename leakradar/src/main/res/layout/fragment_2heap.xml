<?xml version="1.0" encoding="utf-8"?>
<com.jike.uikit.viewgroup.TitleLayout android:id="@+id/titleLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:titlelayout_text="Heap"
    app:titlelayout_title_marginleft="20dp"
    app:titlelayout_title_margintop="42dp"
    app:titlelayout_content_margintop="0dp"
    app:titlelayout_text_size="26sp"
    app:titlelayout_font="@font/inter_bold"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.jike.uikit.viewgroup.OverScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:overscrollview_damping="1.8">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="22dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:background="@drawable/shape_card"
                android:backgroundTint="@color/appLightBlue">



            </FrameLayout>

        </LinearLayout>


    </com.jike.uikit.viewgroup.OverScrollView>


</com.jike.uikit.viewgroup.TitleLayout>