<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/shape_card"
    android:padding="12dp"
    android:backgroundTint="@color/appBlack3"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:textSize="15sp"
        android:textColor="@color/appWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HollowButton"
        android:fontFamily="@font/inter_medium" />

    <com.jike.uikit.view.AppHollowButton
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:uikit_isRound="false"
        app:apphollowbutton_text="Hollow"
        app:apphollowbutton_space="0.1"
        app:uikit_isBackGradient="false"
        app:uikit_backColor="@color/appWhite"
        app:apphollowbutton_textSize="12sp"
        app:uikit_radius="6dp"
        android:layout_marginTop="12dp"
        app:uikit_pressAnimStyle="both"/>

</LinearLayout>
