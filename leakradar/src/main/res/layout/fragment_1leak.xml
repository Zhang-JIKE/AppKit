<?xml version="1.0" encoding="utf-8"?>
<com.jike.uikit.viewgroup.TitleLayout android:id="@+id/titleLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:titlelayout_text="Leaks"
    app:titlelayout_title_marginleft="20dp"
    app:titlelayout_title_margintop="42dp"
    app:titlelayout_content_margintop="0dp"
    app:titlelayout_text_size="26sp"
    app:titlelayout_font="@font/inter_bold"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.jike.uikit.viewgroup.OverScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:overscrollview_damping="1.8">

        <com.jike.uikit.viewgroup.WaterFallLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:waterfalllayout_col="1"
            app:waterfalllayout_space="22dp">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/appLightBlue"
                app:cardCornerRadius="20dp"
                app:cardElevation="0dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:clickable="true"
                    android:background="?attr/selectableItemBackgroundBorderless">

                    <androidx.cardview.widget.CardView
                        android:layout_width="54dp"
                        android:layout_height="54dp"
                        android:layout_marginTop="22dp"
                        android:layout_marginLeft="22dp"
                        android:layout_marginBottom="22dp"
                        app:cardBackgroundColor="@color/appWhite"
                        app:cardCornerRadius="18dp"
                        app:cardElevation="0dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="26"
                            android:textStyle="bold"
                            android:textSize="18sp"
                            android:fontFamily="@font/inter_regular"
                            android:textColor="@color/appBlue"/>

                    </androidx.cardview.widget.CardView>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="22dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="App.context"
                            android:textSize="17.5sp"
                            android:textStyle="bold"
                            android:fontFamily="@font/inter_regular"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Last leaked just now"
                            android:fontFamily="@font/inter_regular"/>

                    </LinearLayout>

                </LinearLayout>



            </androidx.cardview.widget.CardView>


        </com.jike.uikit.viewgroup.WaterFallLayout>


    </com.jike.uikit.viewgroup.OverScrollView>


</com.jike.uikit.viewgroup.TitleLayout>